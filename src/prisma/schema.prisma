generator client {
  provider        = "prisma-client-js"
}

datasource db {
  provider = "mongodb"
  url      = env("DATABASE_URL")
}

model requestLog {
  id        String   @id @default(auto()) @map("_id") @db.ObjectId
  method    String
  url       String
  ip        String
  userAgent String
  status    Int
  duration  Int
  createdAt DateTime @default(now())
  updatedAt DateTime @updatedAt
}
